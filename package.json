{
  "name": "strapi-provider-upload-ibm-object-storage",
  "version": "1.2.0",
  "description": "IBM Cloud Object Storage provider for Strapi upload plugin",
  "license": "Apache-2.0",
  "repository": "github:IBMResearch/strapi-provider-upload-ibm-object-storage",
  "author": "IBM Research - Emerging Technology Experiences <etxservi@us.ibm.com>",
  "homepage": "https://github.com/IBMResearch/strapi-provider-upload-ibm-object-storage",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "deps": "depcheck --ignores='lint-staged,prettier,prettier-plugin-package'",
    "format": "prettier --write . && eslint --fix .",
    "lint": "eslint .",
    "test": "npm run deps && npm run format && npm run lint"
  },
  "files": [
    "index.js"
  ],
  "strapi": {
    "isProvider": true
  },
  "keywords": [
    "upload",
    "ibm",
    "strapi",
    "provider"
  ],
  "dependencies": {
    "ibm-cos-sdk": "^1.10.2"
  },
  "devDependencies": {
    "depcheck": "^1.4.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.3.2"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "arrowParens": "avoid",
    "endOfLine": "lf"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb-base",
      "plugin:node/recommended",
      "plugin:prettier/recommended"
    ],
    "env": {
      "commonjs": true,
      "es6": true,
      "node": true,
      "browser": false,
      "jest": true
    },
    "globals": {
      "strapi": true
    },
    "rules": {
      "strict": [
        "error",
        "global"
      ],
      "prettier/prettier": "warn"
    }
  },
  "eslintIgnore": [
    "**/node_modules/**"
  ]
}
